#contact-wrapper
  position relative
  width 100%
  height 100%
  
  .contact-item
    position absolute
    top 0
    left 0
  
  &:before
    content: ''
    z-index 1
    position absolute
    top 0
    left 0
    background-color rgba(0,0,0,.5)
    width 100%
    height 100%
    
  #ytplayer
    z-index 0
  
  #contact
    z-index 1
    