#contact-wrapper
  position relative
  width 100%
  height 100%
  color: #fff

  .contact-item
    position absolute
    top 0
    left 0

  // Mask
  &:before
    content: ''
    z-index 1
    position absolute
    top 0
    left 0
    background-image url("data:image/png;base64,iVBORw0KGgoAAAANSUhEUgAAABQAAAAUCAMAAAC6V+0/AAAABlBMVEX///8AAABVwtN+AAAAAnRSTlMAmf9A5tgAAAASSURBVHgBY2DEArGCgVI5qhIAV/gAZQ+MArcAAAAASUVORK5CYII=")
    // I'm not the best color-picker person so someone must change this one
    background-color rgba(222, 245, 34, 0.5)
    width 100%
    height 100%

  #ytplayer
    z-index 0

  #contact
    z-index 1
    float left
    align-items center
    justify-content center

    .content
      width 50%
